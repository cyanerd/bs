{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "types": ["vite/client"],
    "paths": {
      "@/public/*": ["./public/*"],
      "@/config/*": ["./config/*"],
      "@/components": ["./src/components/index"],
      "@/components/*": ["./src/components/*"],
      "@/hooks": ["./src/hooks/index"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/page": ["./src/page/index"],
      "@/api": ["./src/api/index"],
      "@/api/*": ["./src/api/*"],
      "@/utils": ["./src/utils/index"],
      "@/utils/*": ["./src/utils/*"],
      "@/types/*": ["./src/types/*"]
    }
  },
  "include": [
    "./src"
  ]
}
